microk8s kubectl apply -n ingress -f - <<EOF
apiVersion: v1
kind: Service
metadata:
  name: nginx-ingress-microk8s
  namespace: ingress
spec:
  type: LoadBalancer
  selector:
    name: nginx-ingress-microk8s
  ports:
    - name: http
      port: 80
      targetPort: 80
    - name: https
      port: 443
      targetPort: 443
EOF
